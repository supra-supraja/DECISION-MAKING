#Budget


a=int(input())
b=int(input())
c=int(input())
d=int(input())
e=int(input())
p=a*350.34
q=b*230.90
r=c*190.55
s=d*125.30
t=e*180.90
z=p+q+r+s+t
if z<=15000:
    print("yes")
else:
    print("no")
    
    
    #Sece Dining
    
    
    x=(input())
y=int(input())
if x=="front" and y==1 or x=="rear"and y==2:
    print("Left Handed")
elif x=="front" and y==2 or x=="rear" and y==1:
    print("Right Handed")
    
    
    #Intersection of a Circle
    
    
    import math
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
d=math.sqrt((x1-x2)**2+(y1-y2)**2)
if d==r1+r2:
    print("The circles are tangential")
elif d<r1+r2:
    print(" The circles overlap")
else:
    print(" The circles do not overlap")



#Microwave Oven


n=int(input())
t=float(input())
print("Enter the number of items")
print("Enter the single item heating time")
c=t+(50/100)*t
d=t+2*t
if n==2:
    print("The recommended  heating time is %.2f"%c)
elif n==3:
    print("The recommended heating time is %.2f"%c)

else:
    print("Number of items is more")#python
    
    
    
