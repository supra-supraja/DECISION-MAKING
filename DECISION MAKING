#Greatest of two numbers


a=int(input())
b=int(input())
if(a==b):
  print("{} is equal to {}".format(a,b))
elif(a>b):
    print("{} is greater than  {}".format(a,b))
elif(a<b):
        print("{} is less than {}".format(a,b))
        


#Vowel Or Consonants


s=input()
if s in "AEIOUaeiou":
    print("vowel")
elif s in"BCDFGHJKLMNPQRSTVWXYZbcdfghjklmnpqrstvwxyz":
    print("consonant")
elif s not in("ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijkmnopqrstuvwxyz"):
    print("not an alphabet")
    
    
    #Grading System
    
    
    marks=int(input())
if marks>=90 and marks<=100:
    print("A")
elif marks>=80 and marks<=90:
    print("B")
elif marks>=70 and marks<=80:
    print("C")
elif marks>=60 and marks<=70:
    print("D")
elif marks>=50 and marks<=60:
    print("E")
elif marks<=50:
    print(F)
elif marks>100:
    print("Invalid Input")
    
    
    #Profit or Loss
    
    
    cp=float(input("Enter the price of a dozen mangoes\n"))
sp=float(input("Enter the price at which 1 mango is being sold\n"))
tsp=sp*12
if tsp<cp:
    print("loss : Rs.%.2f"%(cp-tsp))
elif tsp>cp:
    print("profit : Rs..%.2f"%(tsp-cp))
elif tsp==cp:
     print("no profit nor loss")
     
     
     #Fee Collection
     
     
     print("Enter the student type")
s=input()
if s=="MSDS":
    print("Enter  tuition fee")
    T=float(input())
    print("Enter  Bus fee")
    B=float(input())
    x=T+B
    print("The fees to be paid by the student is Rs.","%.2f"%x)
elif s=="MSH":
    print("Enter tuition fee")
    T=float(input())
    print("Enter Hostel fee")
    H=float(input())
    y=T+H
    print("The fees to be paid by the student is Rs.","%.2f"%y)
elif s=="MGSDS":
    print("Enter tuition fee")
    T=float(input())
    print("Enter  Bus fee")
    B=float(input())
    z=((150/100)*T)+B
    print("The fees to be paid by the student is Rs.","%.2f"%z)
elif s=="MGSH":
    print("Enter tuition fee")
    T=float(input())
    print("Enter Hostel fee")
    H=float(input())
    v=((150/100)*T)+H
  
    print("The fees to be paid by the student is Rs.","%.2f"%v)
    
    
    #Y2K Problem Detector
    
    
    print("Enter year of birth")
by=int(input())
print("Enter current year")
cy=int(input())
py=cy-by
if cy<by:
    by=100-by
    x=by+cy
    print("your age is",x)
else:
    print("your age is",py)
    
    
    #Lab Allocation
    
    
    print("Enter x")
L1=int(input())
print("Enter y")
L2=int(input())
print("Enter z")
L3=int(input())
if L1<L2 and L1<L3:
    print("L1 has the minimal seating capacity")
elif L2<L3:
    print("L2 has the minimal seating capacity")
else:
    print("L3 has the minimal seating capacity")
    
    

